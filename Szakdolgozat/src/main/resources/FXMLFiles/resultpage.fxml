<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="640.0" prefWidth="480.0" style="-fx-background-color: #303960;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ErzelmiIntelligencia.Controllers.ResultController">
    <children>
        <Label layoutX="135.0" layoutY="52.0" text="Eredményed" textAlignment="CENTER" textFill="#ea9a96">
            <font>
                <Font name="System Bold" size="36.0" />
            </font>
        </Label>
        <TextArea fx:id="feedback_text" editable="false" layoutX="57.0" layoutY="241.0" prefHeight="107.0" prefWidth="367.0" visible="false" wrapText="true">
            <font>
                <Font name="System Italic" size="18.0" />
            </font>
        </TextArea>
        <Button fx:id="new_game" layoutX="199.0" layoutY="387.0" mnemonicParsing="false" onMouseClicked="#new_game" text="Új játék" textAlignment="CENTER" visible="false">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button fx:id="quit_button" layoutX="200.0" layoutY="487.0" mnemonicParsing="false" onMouseClicked="#quit" text="Kilépés" visible="false">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
      <TextField fx:id="result_field" alignment="CENTER" editable="false" layoutX="121.0" layoutY="178.0" visible="false">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></TextField>
      <Button fx:id="showButton" layoutX="192.0" layoutY="438.0" mnemonicParsing="false" onMouseClicked="#show_result" text="Mutasd">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
